{
  "name": "aletheia",
  "version": "0.0.1",
  "description": "Crowd-sourced fact-checking platform.",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon server.js",
