{
    "name": "aletheia",
    "version": "0.0.1",
    "description": "Crowd-sourced fact-checking platform.",
    "private": false,
    "license": "MIT",
    "main": "./dist/server/main.js",
    "scripts": {
        "start": "yarn serve",
        "serve": "service-runner",
        "dev": "yarn copy-static-assets && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
        "test": "PREQ_CONNECT_TIMEOUT=15 mocha -r ts-node/register tests/**/*.ts",
        "coverage": "PREQ_CONNECT_TIMEOUT=15 nyc --reporter=lcov _mocha -r ts-node/register tests/**/*.ts",
        "seed": "node ./deprecated/server/infra/database/seeds/generateSeeds.js",
        "seedUser:test": "SEED_TEST_ENVIRONMENT=true ts-node ./deprecated/server/infra/database/seeds/users/seedUsers.js",
        "seedUser": "ts-node ./deprecated/server/infra/database/seeds/users/seedUsers.js",
        "build": "yarn build-ts && yarn build-nest && yarn copy-static-assets && next build",
        "build-ts": "tsc",
        "build-nest": "ENVIRONMENT=watch-dev nest build -p ./server/tsconfig.json",
        "watch-ts": "tsc --project ./server/tsconfig.json -w",
        "debug": "yarn build && yarn watch-debug",
        "lint": "tsc --noEmit && yarn lint:check",
        "lint:check": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" --quiet --fix",
        "copy-static-assets": "ts-node scripts/copyStaticAssets.ts",
        "watch-debug": "yarn copy-static-assets && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
        "watch-node": "nodemon dist/server/server.js  --signal SIGTERM",
        "serve-debug": "nodemon --inspect dist/server/server.js  --signal SIGTERM",
        "clean": "rimraf **/{node_modules,dist}"
    },
    "nodemonConfig": {
        "ignore": [
            "test/**/*",
            "src/**/*",
            "dist/client/**/*"
        ]
    },
    "keywords": [
        "aletheia",
        "REST",
        "API",
        "react",
        "fact-checking"
    ],
    "contributors": [
        {
            "name": "Mateus Santos",
            "email": "mateusbatistasantos@gmail.com",
            "url": "https://github.com/mateusbs17"
        }
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "dependencies": {
        "@ant-design/icons": "^4.6.2",
        "@ant-design/icons-svg": "^4.1.0",
        "@babel/polyfill": "^7.12.1",
        "@nestjs/axios": "^0.0.1",
        "@nestjs/common": "^8.0.6",
        "@nestjs/config": "^1.0.1",
        "@nestjs/core": "^8.0.6",
        "@nestjs/mongoose": "^8.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.6",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "antd": "^4.16.11",
        "axios": "^0.21.2",
        "cookie-parser": "^1.4.5",
        "domino": "^2.1.6",
        "draft-js": "^0.11.7",
        "draft-js-export-html": "^1.4.1",
        "draft-js-import-html": "^1.4.1",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "i18next": "^20.4.0",
        "i18next-browser-languagedetector": "^6.1.2",
        "md5": "^2.3.0",
        "mongoose": "^  5.13.5",
        "next": "^11.1.1",
        "next-i18next": "^8.6.0",
        "next-plugin-antd-less": "^1.3.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.1.0",
        "react": "^17.0.2",
        "react-countdown": "^2.3.2",
        "react-dom": "^17.0.2",
        "react-google-recaptcha": "^2.1.0",
        "react-i18next": "^11.11.4",
        "react-is": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-share": "^4.4.0",
        "react-social-icons": "^5.5.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.3.0",
        "service-runner": "^2.8.4",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@nestjs/cli": "7.6.0",
        "@nestjs/schematics": "7.3.1",
        "@next/eslint-plugin-next": "^11.0.1",
        "@types/eslint": "^7.28.0",
        "@types/express": "^4.17.13",
        "@types/md5": "^2.3.1",
        "@types/node": "^16.4.13",
        "@types/passport-local-mongoose": "^4.0.15",
        "@types/react": "^17.0.16",
        "@typescript-eslint/parser": "^4.29.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-styled-components": "^1.13.2",
        "concurrently": "^6.2.1",
        "eslint": "7.0.0",
        "eslint-config-next": "^11.0.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.9.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.8",
        "husky": "^7.0.1",
        "nodemon": "^2.0.12",
        "prettier": "2.3.2",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "resolutions": {
        "**/**/acorn": ">=5.7.4 <6.0.0 || >=6.4.1 <7.0.0 || >=7.1.1",
        "**/**/minimist": ">=1.2.3",
        "**/**/yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2"
    }
}
