{
    "name": "aletheia",
    "version": "0.0.1",
    "description": "Crowd-sourced fact-checking platform.",
    "private": false,
    "license": "MIT",
    "main": "./dist/server/app.js",
    "scripts": {
        "start": "yarn serve",
        "serve": "service-runner",
        "nest": "nest",
        "dev:nest": "nest start -p tsconfig.nest.json -w",
        "dev:server": "yarn copy-static-assets && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
        "dev:front": "parcel ./src/index.html --out-dir dist/client",
        "test": "PREQ_CONNECT_TIMEOUT=15 mocha -r ts-node/register tests/**/*.ts",
        "coverage": "PREQ_CONNECT_TIMEOUT=15 nyc --reporter=lcov _mocha -r ts-node/register tests/**/*.ts",
        "seed": "node ./server/infra/database/seeds/generateSeeds.js",
        "seedUser:test": "SEED_TEST_ENVIRONMENT=true ts-node ./server/infra/database/seeds/users/seedUsers.js",
        "seedUser": "ts-node ./server/infra/database/seeds/users/seedUsers.js",
        "build": "parcel build ./src/index.html --out-dir dist/assets --no-minify --public-url '/assets' && yarn build-ts && yarn copy-static-assets",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "debug": "yarn build && yarn watch-debug",
        "build:front": "",
        "lint": "tsc --noEmit && yarn lint:check",
        "lint:check": "eslint \"**/*.{js,ts}\"",
        "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node scripts/copyStaticAssets.ts",
        "watch-debug": "yarn copy-static-assets && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
        "watch-node": "nodemon dist/server/server.js  --signal SIGTERM",
        "serve-debug": "nodemon --inspect dist/server/server.js  --signal SIGTERM",
        "clean": "rimraf **/{node_modules,dist}"
    },
    "nodemonConfig": {
        "ignore": [
            "test/**/*",
            "src/**/*",
            "dist/client/**/*"
        ]
    },
    "keywords": [
        "aletheia",
        "REST",
        "API",
        "react",
        "fact-checking"
    ],
    "contributors": [
        {
            "name": "Mateus Santos",
            "email": "mateusbatistasantos@gmail.com",
            "url": "https://github.com/mateusbs17"
        }
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "dependencies": {
        "@ant-design/icons": "4.0.0",
        "@ant-design/icons-svg": "^4.0.0",
        "@babel/polyfill": "^7.8.3",
        "@nestjs/common": "^7.6.17",
        "@nestjs/core": "^7.6.17",
        "@nestjs/mongoose": "^7.2.4",
        "@nestjs/platform-express": "^7.6.17",
        "@reduxjs/toolkit": "^1.3.6",
        "@shelf/jest-mongodb": "^1.0.1",
        "antd": "^4.0.0",
        "axios": "^0.21.1",
        "bootstrap": "^4.4.1",
        "cookie-parser": "^1.4.5",
        "domino": "^2.1.0",
        "draft-js": ">=0.10.0",
        "draft-js-export-html": "^1.4.1",
        "draft-js-import-html": "^1.4.1",
        "express": "^4.16.4",
        "express-session": "^1.17.1",
        "handlebars": "^4.7.7",
        "i18next": ">= 19.0.0",
        "i18next-browser-languagedetector": "^4.2.0",
        "immutable": "3.x.x",
        "jquery": "1.9.1 - 3",
        "js-yaml": "^3.12.0",
        "md5": "^2.2.1",
        "mocha": "^7.1.2",
        "moment": "^2.29.1",
        "mongoose": "^5.7.5",
        "mongoose-seed": "^0.6.0",
        "nodemailer": "^6.6.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.1.0",
        "popper.js": "^1.16.0",
        "react": ">= 16.8.0",
        "react-bootstrap-typeahead": "^4.0.0-rc.2",
        "react-dom": "^0.14.0 || ^15.0.0-rc || ^16.0.0-rc || ^16.0.0",
        "react-google-recaptcha": "^2.0.1",
        "react-i18next": "^11.5.0",
        "react-paginate": "^6.3.2",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "react-share": "^4.4.0",
        "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
        "redux-thunk": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.1.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.2",
        "service-runner": "^2.7.6",
        "store": "^2.0.12",
        "styled-components": "^5.0.1",
        "swagger-ui-dist": "^3.22.3",
        "underscore": "^1.12.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@nestjs/testing": "^7.6.17",
        "@types/body-parser": "^1.19.0",
        "@types/chai": "^4.2.11",
        "@types/concurrently": "^5.2.0",
        "@types/eslint": "^6.8.0",
        "@types/express": "^4.17.12",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.14.36",
        "@types/shelljs": "^0.8.7",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "babel-eslint": "^10.0.3",
        "chai": "^4.2.0",
        "concurrently": "^5.2.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-react-app": "^5.2.0",
        "eslint-config-wikimedia": "^0.15.0",
        "eslint-plugin-flowtype": "^4.6.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsdoc": "^21.0.0",
        "eslint-plugin-json": "^2.0.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^2.3.0",
        "husky": "^2.4.1",
        "jest": "^27.0.1",
        "less": "^3.11.1",
        "nodemon": "^2.0.3",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.3.0",
        "rimraf": "3.0.2",
        "shelljs": "^0.8.4",
        "supertest": "^6.1.3",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "resolutions": {
        "**/**/acorn": ">=5.7.4 <6.0.0 || >=6.4.1 <7.0.0 || >=7.1.1",
        "**/**/minimist": ">=1.2.3",
        "**/**/yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2"
    }
}
