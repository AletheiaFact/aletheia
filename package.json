  "keywords": [
    "aletheia",
    "REST",
    "API",
    "react",
    "fact-checking"
  ],
  "contributors": [
    {
      "name": "Mateus Santos",
      "email": "mateusbatistasantos@gmail.com",
      "url": "https://github.com/mateusbs17"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@shelf/jest-mongodb": "^1.0.1",
    "axios": "^0.19.2",
    "domino": "^2.1.0",
    "express": "^4.16.4",
    "js-yaml": "^3.12.0",
    "md5": "^2.2.1",
