{
    "displayName": "server",
    "moduleFileExtensions": [
        "js",
        "json",
        "ts",
        "tsx"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
        "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "modulePathIgnorePatterns": ["./dist/", "./tests/"]
}
